<h1>投稿一覧</h1>
 <% @posts.each do |post| %>
    <p>タイトル</p>
  <span><%= post.title %></span>
  <p>画像</p>
  <%= attachment_image_tag post, :image, :fill, 100, 100, format: 'jpeg' %>

     <span>
        <%=link_to post.title, post_path(post.id) , :style => 'border:1px solid green' %>
    </span>
<% end %>
